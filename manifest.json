{
  "name": "QR Code",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "QR Code generator for web page",
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "QR Code",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
